{
	"name": "gourmet/platform"
	, "description": "Platform for CakePHP 3 web applications"
	, "version": "0.0.0"
	, "type": "project"
	, "keywords": [
		"cakephp"
		, "gourmet"
		, "app"
		, "platform"
	]
	, "homepage": "http://github.com/gourmet/platform"
	, "license": "MIT"
	, "authors": [
		{
			"name": "Jad Bitar"
			, "homepage": "http://jadb.io"
			, "role": "Author"
		}
	]
	, "support": {
		"issues": "http://github.com/gourmet/platform/issues"
		, "source": "http://github.com/gourmet/platform"
	}
	, "require": {
		"php": ">=5.4.19"
		, "cakephp/cakephp": "3.0.*-dev"
		, "cakephp/debug_kit": "3.0.*-dev"
		, "cakephp/migrations": "dev-master"
		, "gourmet/assetic": "~0.2.0"
		, "gourmet/robo": "~0.2.0"
		, "friendsofcake/crud": "dev-cake3"
		, "fzaninotto/faker": "~1.3"
		, "mobiledetect/mobiledetectlib": "~2.0"
		, "psr/log": "~1.0"
	}
	, "require-dev": {
		"codeception/codeception": "~2.0"
		, "codeception/specify": "~0.3"
		, "codeception/verify": "~0.2"
		, "d11wtq/boris": "~1.0.0"
		, "gourmet/whoops": "~0.1.0"
	}
	, "autoload": {
		"psr-4": {
			"App\\": "src"
			, "App\\Build\\": "build"
            , "App\\Test\\": "tests"
            , "App\\Test\\Codeception\\Module\\": "tests/_helpers"
		}
	}
	, "minimum-stability": "dev"
	, "config": {
		"preferred-install": "dist"
		, "bin-dir": "bin"
	}
	, "extra": {
		"installer-paths": {
			"plugin/{$name}": ["type:cakephp-plugin"]
		}
	}
}
